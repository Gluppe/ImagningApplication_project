<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<VBox prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <BorderPane prefHeight="779.0" prefWidth="1076.0">
      <top>
         <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
               <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                  <children>
                     <Button mnemonicParsing="false" onAction="#btnHome" text="Home" />
                  </children>
               </VBox>

               <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                  <children>
                     <Button mnemonicParsing="false" onAction="#btnGallery" text="Gallery" />
                  </children>
               </VBox>
               <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                  <children>
                     <Button mnemonicParsing="false" onAction="#btnAddImage" text="Add Image" />
                  </children>
               </VBox>
            </children>
         </HBox>
      </top>
      <center>
         <HBox prefHeight="464.0" prefWidth="1076.0" BorderPane.alignment="CENTER">
            <children>
               <VBox alignment="CENTER" prefHeight="603.0" prefWidth="550.0">
                  <children>
                     <Text fx:id="imageName" strokeType="OUTSIDE" strokeWidth="0.0" text="Filename" />
                     <ImageView fx:id="imageView" fitHeight="476.0" fitWidth="515.0" pickOnBounds="true" preserveRatio="true" />
                     <Text fx:id="tags" strokeType="OUTSIDE" strokeWidth="0.0" text="tags" />
                  </children>
               </VBox>
               <VBox alignment="CENTER" prefHeight="603.0" prefWidth="530.0">
                  <children>
                     <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Metadata" />
                     <ScrollPane prefHeight="473.0" prefWidth="528.0">
                        <content>
                           <TextFlow fx:id="textFlow" prefHeight="505.0" prefWidth="508.0" />
                        </content></ScrollPane>
                     <Button onAction="#btnDelete" mnemonicParsing="false" text="Remove Image From Gallery">
                        <VBox.margin>
                           <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </VBox.margin>
                     </Button>
                  </children>
               </VBox>
            </children>
         </HBox>
      </center>
   </BorderPane>
</VBox>
