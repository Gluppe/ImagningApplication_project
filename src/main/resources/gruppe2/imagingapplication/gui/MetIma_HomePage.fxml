<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:controller="gruppe2.imagingapplication.gui.HomepageController" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
  <stylesheets>
    <URL value="@BasicApplication.css" />
  </stylesheets>
   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
      <children>
       <Text strokeType="OUTSIDE" strokeWidth="0.0" text="MetIma">
         <font>
           <Font name="System Bold" size="74.0" />
         </font>
       </Text>
      </children>
   </HBox>
   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <children>
         <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
            <children>
             <Button mnemonicParsing="false" text="view gallery" fx:id="galleryBtn" onAction="#viewGallery"/>
             <Text strokeType="OUTSIDE" strokeWidth="0.0" text="view gallery" />
            </children>
         </VBox>
         <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
            <children>
             <Button mnemonicParsing="false" text="add image" fx:id="addImageBtn" />
             <Text strokeType="OUTSIDE" strokeWidth="0.0" text="add image" />
            </children>
         </VBox>
      </children>
   </HBox>
</VBox>
